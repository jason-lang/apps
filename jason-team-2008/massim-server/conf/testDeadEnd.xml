                agentSpeed="1" lineOfSight="17" fearRange="4" attractionRange="5"
                cowWeight="5" agentWeight="-200" emptyWeight="1" obstacleWeight="-1"
                htaccess="1">

                <array item0="35" item1="69" item2="50" item3="34"
