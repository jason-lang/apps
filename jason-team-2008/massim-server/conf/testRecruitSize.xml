                    team="blue">
                    <configuration />
                </agent>
                <agent
                    agentclass="massim.simulation.GridSimulationAgent"
                    agentcreationclass="massim.simulation.GridSimulationAgentParameter"
                    team="blue">
                    <configuration />
                </agent>
                <agent
